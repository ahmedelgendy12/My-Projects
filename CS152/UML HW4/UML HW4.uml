      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_nY24gDzpEeyaIa6IaE-12Q" name="getDouble" specification="_nVsPwDzpEeyaIa6IaE-12Q">
        <ownedParameter xmi:id="_nY24gTzpEeyaIa6IaE-12Q" direction="return">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
        </ownedParameter>
        <language>java</language>
        <body>int count = 0;
 // take user input
 String input = scan.nextLine();
 String acceptDouble = &quot;.0123456789&quot;;
 String result = &quot;&quot;;
 // iterate through the String
 for (int index = 0; index &lt; input.length(); index++) {
   char letter = input.charAt(index);
   // Checks for '.' and if the second one was typed in replace with 'a'
   if (letter == '.') {
     count++;
     if (count > 1) {
       letter = 'a';
     }
   }
   if (acceptDouble.indexOf(letter) > -1) {
     result += letter;
   }
 }
 if (result.charAt(result.length() - 1) == '.') {
   result += '0';
 }

 if (result.equals(&quot;&quot;)) {
   throw problem;
 }
 double finalResult = Double.parseDouble(result);

 return finalResult;
  </body>
      </ownedBehavior>
